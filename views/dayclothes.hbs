{{> navbar }}


<div id="productviewone" class="products-grid flexed-item">

  {{#each dayclothes}}
  {{#each current}}

  <div class="product-item-wrapper">
    <img class="product-img" src="{{this.image}}" alt="cloth">
    <p class="product-name">{{this.name}}</p>
    <p class="product-name">{{this.type}}</p>
    <p class="product-name">{{this.brand}}</p>
    <p class="product-price">${{this.price}}</p>
  </div>
  {{/each}}
  {{/each}}
</div>

{{> footer}}